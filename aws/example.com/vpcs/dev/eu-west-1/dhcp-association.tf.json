{
  "data": {
    "aws_vpc_dhcp_options": {
      "this": {
        "dynamic": {
          "filter": {
            "content": {
              "name": "${format(\"tag:%s\", filter.key)}",
              "values": "${[filter.value]}"
            },
            "for_each": {
              "Domain": "example.com",
              "Environment": "dev",
              "Name": "dev"
            }
          }
        }
      }
    }
  },
  "resource": {
    "aws_vpc_dhcp_options_association": {
      "this": {
        "dhcp_options_id": "${data.aws_vpc_dhcp_options.this.id}",
        "vpc_id": "${aws_vpc.this.id}"
      }
    }
  }
}
